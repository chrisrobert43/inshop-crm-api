{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.4.5",
        "ext-iconv": "*",
        "ext-json": "*",
        "api-platform/api-pack": "^1.3",
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "doctrine/doctrine-migrations-bundle": "^3.0",
        "gesdinet/jwt-refresh-token-bundle": "^0.9",
        "google/apiclient": "^2.8",
        "jms/serializer-bundle": "^3.7",
        "league/flysystem-aws-s3-v3": " ^1.0.26",
        "lexik/jwt-authentication-bundle": "^2.10",
        "liip/imagine-bundle": "^2.3",
        "oneup/flysystem-bundle": "^3.7",
        "ruflin/elastica": "^7.0",
        "sensio/framework-extra-bundle": "^5.6",
        "sentry/sentry-symfony": "^3.5",
        "antishov/doctrine-extensions-bundle": "^1.4",
        "symfony/console": "^5.2",
        "symfony/flex": "^1.11",
        "symfony/translation": "^5.2",
        "symfony/framework-bundle": "^5.2",
        "symfony/http-client": "^5.2",
        "symfony/monolog-bundle": "^3.6",
        "symfony/security-bundle": "^5.2",
        "symfony/swiftmailer-bundle": "^3.5",
        "symfony/templating": "^5.2",
        "symfony/twig-bundle": "^5.2",
        "symfony/validator": "^5.2",
        "symfony/yaml": "^5.2",
        "vich/uploader-bundle": "^1.16",
        "fakerphp/faker": "^1.13"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "symfony/dotenv": "^5.2",
        "symfony/maker-bundle": "^1.25",
        "symfony/profiler-pack": "^1.0",
        "symfony/var-dumper": "^5.2"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false
        }
    }
}
